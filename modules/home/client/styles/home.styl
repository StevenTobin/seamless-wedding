.home
    .names
        float: left
        margin-top: 75px
        margin-left: 50px

    .image-sequence
        li
            list-style-type: none

    .image-sequence
        li
            span
                width: 495px
                margin-left: 42px
                height: 605px
                margin-top: 10px
                position: absolute
                color: transparent
                opacity: 0
                -webkit-backface-visibility: hidden
                -webkit-animation: imageAnimation 14s linear infinite 1ms
                -moz-animation: imageAnimation 14s linear infinite 1ms
                -o-animation: imageAnimation 14s linear infinite 1ms
                -ms-animation: imageAnimation 14s linear infinite 1ms
                animation: imageAnimation 14s linear infinite 1ms

    @keyframes imageAnimation
        0%
            opacity: 0
            animation-timing-function: ease-in
        100%
            opacity: 1
            animation-timing-function: ease-out

    .image-sequence
        li:nth-child(1)
            span
                background-image: url(../images/home/sequence/falling-leaves-1.jpg)
                background-size: 495px auto

    .image-sequence
        li:nth-child(2)
            span
                background-image: url(../images/home/sequence/falling-leaves-2.jpg)
                background-size: 495px auto
                -webkit-animation-delay: 2s;
                -moz-animation-delay: 2s;
                -o-animation-delay: 2s;
