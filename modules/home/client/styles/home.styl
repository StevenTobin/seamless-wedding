.home
    .names
        margin-top: 100px
        margin-left: 80px
        float: left

    .image-sequence
        li
            list-style-type: none
            span
                width: 423px
                margin-left: 147px
                height: content-height - 30
                margin-top: 15px
                position: absolute
                color: transparent
                opacity: 0
                -webkit-backface-visibility: hidden
                -webkit-animation: imageAnimation 14s linear infinite 0s
                -moz-animation: imageAnimation 14s linear infinite 0s
                -o-animation: imageAnimation 14s linear infinite 0s
                -ms-animation: imageAnimation 14s linear infinite 0s
                animation: imageAnimation 14s linear infinite 0s

    @keyframes imageAnimation
        0%
            opacity: 0
        7%
            opacity: 1
        16%
            opacity: 1
        28%
            opacity: 0
        100%
            opacity: 0

    .image-sequence
        li:nth-child(1)
            span
                background-image: url(../images/home/sequence/falling-leaves-1.jpg)
                background-size: auto content-height - 30

    .image-sequence
        li:nth-child(2)
